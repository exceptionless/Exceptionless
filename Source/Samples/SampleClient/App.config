<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="exceptionless" type="Exceptionless.Configuration.ExceptionlessSection, Exceptionless"/>
  </configSections>

  <exceptionless apiKey="e3d51ea621464280bbcb79c11fd6483e" serverUrl="http://localhost:40000" enableSSL="false"/>

  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="textfileListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="trace.log"/>
      </listeners>
    </trace>

    <switches>
      <!--
        0 - Disabled
        1 - Gives error messages
        2 - Gives errors and warnings
        3 - Gives more detailed error information
        4 - Gives verbose trace information
        -->
      <add name="ExceptionlessSwitch" value="3"/>
    </switches>
  </system.diagnostics>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
  </startup>
</configuration>