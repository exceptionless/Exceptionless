{
    "name": "exceptionless",
    "version": "8.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--trace-warnings vite dev",
        "dev:api": "cross-env ASPNETCORE_URLS=https://be.exceptionless.io/ vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "run-s -c test:unit test:integration",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "run-s -c lint:prettier lint:eslint",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check .",
        "format": "run-s -c format:prettier format:eslint",
        "format:eslint": "eslint . --fix",
        "format:prettier": "prettier --write .",
        "generate-models": "swagger-typescript-api -p http://localhost:5200/docs/v2/swagger.json -o ./src/lib/generated -n api.ts --no-client --templates api-templates",
        "generate-templates": "swagger-typescript-api generate-templates -o api-templates",
        "test:integration": "playwright test",
        "test:unit": "vitest",
        "upgrade": "ncu -i"
    },
    "devDependencies": {
        "@playwright/test": "^1.46.1",
        "@sveltejs/adapter-static": "^3.0.4",
        "@sveltejs/kit": "^2.5.24",
        "@sveltejs/vite-plugin-svelte": "^3.1.2",
        "@types/eslint": "^9.6.0",
        "autoprefixer": "^10.4.20",
        "cross-env": "^7.0.3",
        "eslint": "^9.9.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-perfectionist": "^3.2.0",
        "eslint-plugin-svelte": "^2.43.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.41",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.6",
        "prettier-plugin-tailwindcss": "^0.6.6",
        "svelte": "^5.0.0-next.234",
        "svelte-check": "^3.8.6",
        "swagger-typescript-api": "^13.0.21",
        "tslib": "^2.6.3",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.2.0",
        "vite": "^5.4.2",
        "vitest": "^2.0.5"
    },
    "dependencies": {
        "@exceptionless/browser": "^3.1.0",
        "@exceptionless/fetchclient": "^0.27.0",
        "@iconify-json/mdi": "^1.1.68",
        "@tanstack/svelte-query": "https://pkg.pr.new/@tanstack/svelte-query@7bccb14",
        "@tanstack/svelte-query-devtools": "https://pkg.pr.new/@tanstack/svelte-query-devtools@7bccb14",
        "@tanstack/svelte-table": "^9.0.0-alpha.10",
        "@typeschema/class-validator": "^0.1.2",
        "bits-ui": "^0.21.13",
        "class-validator": "^0.14.1",
        "clsx": "^2.1.1",
        "cmdk-sv": "^0.0.18",
        "formsnap": "^1.0.1",
        "mode-watcher": "^0.4.1",
        "oidc-client-ts": "^3.0.1",
        "pretty-ms": "^9.1.0",
        "runed": "^0.15.1",
        "svelte-persisted-store": "^0.11.0",
        "svelte-sonner": "^0.3.27",
        "svelte-time": "^0.9.0",
        "sveltekit-superforms": "^2.17.0",
        "tailwind-merge": "^2.5.2",
        "tailwind-variants": "^0.2.1",
        "tailwindcss": "^3.4.10",
        "unplugin-icons": "^0.19.2"
    },
    "type": "module"
}
