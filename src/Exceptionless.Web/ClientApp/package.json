{
    "name": "exceptionless",
    "version": "8.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--trace-warnings vite dev",
        "dev:api": "cross-env ASPNETCORE_URLS=https://be.exceptionless.io/ vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "npm run lint:prettier && npm run lint:eslint",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check .",
        "format": "npm run format:prettier && npm run format:eslint",
        "format:eslint": "eslint . --fix",
        "format:prettier": "prettier --write .",
        "generate-models": "swagger-typescript-api -p http://localhost:5200/docs/v2/swagger.json -o ./src/lib/generated -n api.ts --no-client --templates api-templates",
        "generate-templates": "swagger-typescript-api generate-templates -o api-templates",
        "test:e2e": "playwright test",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "upgrade": "ncu -i"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.7",
        "@eslint/js": "^9.21.0",
        "@iconify-json/lucide": "^1.2.26",
        "@playwright/test": "^1.50.1",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.17.2",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@tailwindcss/vite": "^4.0.8",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/svelte": "^5.2.7",
        "@types/eslint": "^9.6.1",
        "@types/node": "^22.13.5",
        "@types/throttle-debounce": "^5.0.2",
        "cross-env": "^7.0.3",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-perfectionist": "^4.9.0",
        "eslint-plugin-svelte": "^2.46.1",
        "jsdom": "^26.0.0",
        "prettier": "^3.5.1",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "svelte": "^5.20.2",
        "svelte-check": "^4.1.4",
        "swagger-typescript-api": "^13.0.23",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.24.1",
        "vite": "^6.1.1",
        "vitest": "3.0.6"
    },
    "dependencies": {
        "@exceptionless/browser": "^3.1.0",
        "@exceptionless/fetchclient": "^0.40.0",
        "@iconify-json/mdi": "^1.2.3",
        "@tanstack/svelte-query": "https://pkg.pr.new/@tanstack/svelte-query@28f98f9",
        "@tanstack/svelte-query-devtools": "https://pkg.pr.new/@tanstack/svelte-query-devtools@28f98f9",
        "@tanstack/svelte-table": "^9.0.0-alpha.10",
        "@typeschema/class-validator": "^0.3.0",
        "bits-ui": "^1.3.2",
        "class-validator": "^0.14.1",
        "clsx": "^2.1.1",
        "formsnap": "^2.0.0",
        "kit-query-params": "^0.0.26",
        "lucide-svelte": "^0.475.0",
        "mode-watcher": "^0.5.1",
        "oidc-client-ts": "^3.1.0",
        "pretty-ms": "^9.2.0",
        "runed": "^0.23.4",
        "svelte-sonner": "^0.3.28",
        "svelte-time": "^2.0.0",
        "sveltekit-superforms": "^2.23.1",
        "tailwind-merge": "^3.0.1",
        "tailwind-variants": "^0.3.1",
        "tailwindcss": "^4.0.8",
        "throttle-debounce": "^5.0.2",
        "unplugin-icons": "^22.1.0"
    },
    "type": "module"
}
