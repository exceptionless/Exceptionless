{
	"name": "exceptionless",
	"version": "8.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
		"format": "prettier --plugin prettier-plugin-svelte . --write .",
		"format:staged": "prettier --plugin prettier-plugin-svelte . --write --ignore-unknown",
		"generate-models": "swagger-typescript-api -p http://localhost:5200/docs/v2/swagger.json -o ./src/lib/models -n api.generated.ts --no-client",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"dependencies": {
		"@iconify-json/mdi": "^1.1.54",
		"@sveltestack/svelte-query": "^1.6.0",
		"class-validator": "^0.14.0",
		"daisyui": "^3.5.1",
		"oidc-client-ts": "^2.2.5",
		"svelte-local-storage-store": "^0.6.0",
		"tailwindcss": "^3.3.3",
		"unplugin-icons": "^0.16.5"
	},
	"devDependencies": {
		"@playwright/test": "^1.37.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.22.6",
		"@sveltejs/vite-plugin-svelte": "^2.4.5",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^6.4.0",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.32.4",
		"postcss": "^8.4.28",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.3",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"swagger-typescript-api": "^13.0.3",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.9",
		"vitest": "^0.34.2"
	},
	"type": "module"
}
