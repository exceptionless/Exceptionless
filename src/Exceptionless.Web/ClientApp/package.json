{
    "name": "exceptionless",
    "version": "8.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--trace-warnings vite dev",
        "dev:api": "cross-env ASPNETCORE_URLS=https://be.exceptionless.io/ vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "npm run lint:prettier && npm run lint:eslint",
        "lint:eslint": "eslint . --concurrency=auto",
        "lint:prettier": "prettier --check .",
        "format": "npm run format:prettier && npm run format:eslint",
        "format:eslint": "eslint . --fix --concurrency=auto",
        "format:prettier": "prettier --write .",
        "generate-models": "node scripts/generate-api.mjs",
        "generate-templates": "swagger-typescript-api generate-templates -o api-templates",
        "test:e2e": "playwright test",
        "test:unit": "vitest run",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "upgrade": "ncu -i"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^4.1.3",
        "@eslint/compat": "^2.0.0",
        "@eslint/js": "^9.39.2",
        "@iconify-json/lucide": "^1.2.82",
        "@playwright/test": "^1.57.0",
        "@storybook/addon-a11y": "^10.1.11",
        "@storybook/addon-docs": "^10.1.11",
        "@storybook/addon-svelte-csf": "^5.0.10",
        "@storybook/sveltekit": "^10.1.11",
        "@sveltejs/adapter-static": "^3.0.10",
        "@sveltejs/kit": "^2.49.2",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/vite": "^4.1.18",
        "@tanstack/eslint-plugin-query": "^5.91.2",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/svelte": "^5.3.1",
        "@types/eslint": "^9.6.1",
        "@types/node": "^25.0.3",
        "@types/throttle-debounce": "^5.0.2",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-perfectionist": "^5.2.0",
        "eslint-plugin-storybook": "^10.1.11",
        "eslint-plugin-svelte": "^3.13.1",
        "jsdom": "^27.4.0",
        "prettier": "^3.7.4",
        "prettier-plugin-svelte": "^3.4.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "storybook": "^10.1.11",
        "svelte": "^5.46.1",
        "svelte-check": "^4.3.5",
        "swagger-typescript-api": "^13.2.16",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.51.0",
        "vite": "^7.3.0",
        "vitest": "4.0.16",
        "vitest-websocket-mock": "^0.5.0",
        "zod": "^4.2.1"
    },
    "dependencies": {
        "@exceptionless/browser": "^3.1.0",
        "@exceptionless/fetchclient": "^0.44.0",
        "@internationalized/date": "^3.10.1",
        "@lucide/svelte": "^0.562.0",
        "@tanstack/svelte-form": "^1.27.7",
        "@tanstack/svelte-query": "^6.0.13",
        "@tanstack/svelte-query-devtools": "^6.0.3",
        "@tanstack/svelte-table": "^9.0.0-alpha.10",
        "@types/d3-scale": "^4.0.9",
        "@types/d3-shape": "^3.1.7",
        "bits-ui": "^2.14.4",
        "clsx": "^2.1.1",
        "d3-scale": "^4.0.2",
        "dompurify": "^3.3.1",
        "kit-query-params": "^0.0.26",
        "layerchart": "^2.0.0-next.43",
        "mode-watcher": "^1.1.0",
        "oidc-client-ts": "^3.4.1",
        "pretty-ms": "^9.3.0",
        "runed": "^0.37.1",
        "shiki": "^3.20.0",
        "svelte-sonner": "^1.0.7",
        "svelte-time": "^2.0.2",
        "tailwind-merge": "^3.4.0",
        "tailwind-variants": "^3.2.2",
        "tailwindcss": "^4.1.18",
        "throttle-debounce": "^5.0.2",
        "tw-animate-css": "^1.4.0"
    },
    "type": "module",
    "overrides": {
        "storybook": "$storybook"
    }
}
