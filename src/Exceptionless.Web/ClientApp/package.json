{
    "name": "exceptionless",
    "version": "8.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--trace-warnings vite dev",
        "dev:api": "cross-env ASPNETCORE_URLS=https://be.exceptionless.io/ vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "run-s -c test:unit test:integration",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "run-s -c lint:eslint lint:prettier",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check .",
        "format": "prettier --write .",
        "generate-models": "swagger-typescript-api -p http://localhost:5200/docs/v2/swagger.json -o ./src/lib/models -n api.generated.ts --no-client --templates api-templates",
        "generate-templates": "swagger-typescript-api generate-templates -o api-templates",
        "test:integration": "playwright test",
        "test:unit": "vitest",
        "upgrade": "ncu -i"
    },
    "devDependencies": {
        "@playwright/test": "^1.42.0",
        "@sveltejs/adapter-static": "^3.0.1",
        "@sveltejs/kit": "^2.5.2",
        "@sveltejs/vite-plugin-svelte": "^3.0.2",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "autoprefixer": "^10.4.18",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "prettier-plugin-svelte": "^3.2.2",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "svelte": "^4.2.12",
        "svelte-check": "^3.6.6",
        "swagger-typescript-api": "^13.0.3",
        "tslib": "^2.6.2",
        "typescript": "^5.4.0-beta",
        "vite": "^5.1.4",
        "vitest": "^1.3.1"
    },
    "dependencies": {
        "@exceptionless/browser": "^3.0.5",
        "@iconify-json/mdi": "^1.1.64",
        "@tanstack/svelte-query": "^5.24.1",
        "@tanstack/svelte-query-devtools": "^5.24.1",
        "@tanstack/svelte-table": "^8.13.2",
        "@web3-storage/parse-link-header": "^3.1.0",
        "bits-ui": "^0.18.6",
        "class-validator": "^0.14.1",
        "clsx": "^2.1.0",
        "cmdk-sv": "^0.0.13",
        "echarts": "^5.5.0",
        "mode-watcher": "^0.2.1",
        "oidc-client-ts": "^3.0.1",
        "pretty-ms": "^9.0.0",
        "svelte-echarts": "^0.1.1",
        "svelte-legos": "^0.2.2",
        "svelte-local-storage-store": "^0.6.4",
        "svelte-sonner": "^0.3.19",
        "svelte-time": "^0.8.2",
        "tailwind-merge": "^2.2.1",
        "tailwind-variants": "^0.2.0",
        "tailwindcss": "^3.4.1",
        "unplugin-icons": "^0.18.5"
    },
    "type": "module"
}
