{
    "name": "exceptionless",
    "version": "8.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--trace-warnings vite dev",
        "dev:api": "cross-env ASPNETCORE_URLS=https://be.exceptionless.io/ vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "run-s -c test:unit test:integration",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "run-s -c lint:prettier lint:eslint",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check .",
        "format": "run-s -c format:prettier format:eslint",
        "format:eslint": "eslint . --fix",
        "format:prettier": "prettier --write .",
        "generate-models": "swagger-typescript-api -p http://localhost:5200/docs/v2/swagger.json -o ./src/lib/generated -n api.ts --no-client --templates api-templates",
        "generate-templates": "swagger-typescript-api generate-templates -o api-templates",
        "test:integration": "playwright test",
        "test:unit": "vitest run",
        "upgrade": "ncu -i"
    },
    "devDependencies": {
        "@iconify-json/lucide": "^1.2.22",
        "@playwright/test": "^1.49.1",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.15.2",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@types/eslint": "^9.6.1",
        "@types/node": "^22.10.5",
        "@types/throttle-debounce": "^5.0.2",
        "autoprefixer": "^10.4.20",
        "cross-env": "^7.0.3",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-perfectionist": "^4.6.0",
        "eslint-plugin-svelte": "^2.46.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "svelte": "^5.19.3",
        "svelte-check": "^4.1.3",
        "swagger-typescript-api": "^13.0.23",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.19.1",
        "vite": "^6.0.7",
        "vitest": "2.1.6"
    },
    "dependencies": {
        "@exceptionless/browser": "^3.1.0",
        "@exceptionless/fetchclient": "^0.36.0",
        "@iconify-json/mdi": "^1.2.2",
        "@tanstack/svelte-query": "https://pkg.pr.new/@tanstack/svelte-query@28f98f9",
        "@tanstack/svelte-query-devtools": "https://pkg.pr.new/@tanstack/svelte-query-devtools@28f98f9",
        "@tanstack/svelte-table": "^9.0.0-alpha.10",
        "@typeschema/class-validator": "^0.3.0",
        "bits-ui": "^1.0.0-next.77",
        "class-validator": "^0.14.1",
        "clsx": "^2.1.1",
        "formsnap": "^2.0.0",
        "lucide-svelte": "^0.471.0",
        "mode-watcher": "^0.5.0",
        "oidc-client-ts": "^3.1.0",
        "pretty-ms": "^9.2.0",
        "runed": "^0.23.0",
        "svelte-sonner": "^0.3.28",
        "svelte-time": "^0.9.0",
        "sveltekit-superforms": "^2.22.1",
        "tailwind-merge": "^2.6.0",
        "tailwind-variants": "^0.3.0",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "throttle-debounce": "^5.0.2",
        "unplugin-icons": "^22.0.0"
    },
    "type": "module"
}
