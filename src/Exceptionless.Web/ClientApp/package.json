{
    "name": "exceptionless",
    "version": "8.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--trace-warnings vite dev",
        "dev:api": "cross-env ASPNETCORE_URLS=https://be.exceptionless.io/ vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "npm run lint:prettier && npm run lint:eslint",
        "lint:eslint": "eslint . --concurrency=auto",
        "lint:prettier": "prettier --check .",
        "format": "npm run format:prettier && npm run format:eslint",
        "format:eslint": "eslint . --fix --concurrency=auto",
        "format:prettier": "prettier --write .",
        "generate-models": "swagger-typescript-api -p http://localhost:5200/docs/v2/swagger.json -o ./src/lib/generated -n api.ts --no-client --templates api-templates",
        "generate-templates": "swagger-typescript-api generate-templates -o api-templates",
        "test:e2e": "playwright test",
        "test:unit": "vitest run",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "upgrade": "ncu -i"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^4.1.1",
        "@eslint/compat": "^1.4.0",
        "@eslint/js": "^9.37.0",
        "@iconify-json/lucide": "^1.2.69",
        "@playwright/test": "^1.56.0",
        "@storybook/addon-a11y": "^9.1.10",
        "@storybook/addon-docs": "^9.1.10",
        "@storybook/addon-svelte-csf": "^5.0.10",
        "@storybook/sveltekit": "^9.1.10",
        "@sveltejs/adapter-static": "^3.0.10",
        "@sveltejs/kit": "^2.46.4",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/vite": "^4.1.14",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/svelte": "^5.2.8",
        "@types/eslint": "^9.6.1",
        "@types/node": "^24.7.0",
        "@types/throttle-debounce": "^5.0.2",
        "cross-env": "^10.1.0",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-perfectionist": "^4.15.1",
        "eslint-plugin-storybook": "^9.1.10",
        "eslint-plugin-svelte": "^3.12.4",
        "jsdom": "^27.0.0",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "storybook": "^9.1.10",
        "svelte": "^5.39.11",
        "svelte-check": "^4.3.3",
        "swagger-typescript-api": "^13.2.15",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.0",
        "vite": "^7.1.9",
        "vitest": "3.2.4"
    },
    "dependencies": {
        "@exceptionless/browser": "^3.1.0",
        "@exceptionless/fetchclient": "^0.44.0",
        "@internationalized/date": "^3.10.0",
        "@lucide/svelte": "^0.545.0",
        "@tanstack/svelte-query": "^6.0.0",
        "@tanstack/svelte-query-devtools": "^6.0.0",
        "@tanstack/svelte-table": "^9.0.0-alpha.10",
        "@types/d3-scale": "^4.0.9",
        "@types/d3-shape": "^3.1.7",
        "@typeschema/class-validator": "^0.3.0",
        "bits-ui": "^2.11.4",
        "class-validator": "^0.14.2",
        "clsx": "^2.1.1",
        "d3-scale": "^4.0.2",
        "dompurify": "^3.2.7",
        "formsnap": "^2.0.1",
        "kit-query-params": "^0.0.26",
        "layerchart": "^2.0.0-next.40",
        "mode-watcher": "^1.1.0",
        "oidc-client-ts": "^3.3.0",
        "pretty-ms": "^9.3.0",
        "runed": "^0.34.0",
        "shiki": "^3.13.0",
        "svelte-sonner": "^1.0.5",
        "svelte-time": "^2.0.1",
        "sveltekit-superforms": "^2.28.0",
        "tailwind-merge": "^3.3.1",
        "tailwind-variants": "^3.1.1",
        "tailwindcss": "^4.1.14",
        "throttle-debounce": "^5.0.2",
        "tw-animate-css": "^1.4.0"
    },
    "type": "module",
    "overrides": {
        "storybook": "$storybook"
    }
}
