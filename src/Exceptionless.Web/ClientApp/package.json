{
	"name": "exceptionless",
	"version": "8.0.0",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_OPTIONS=--trace-warnings vite dev",
		"dev:api": "cross-env ASPNETCORE_URLS=http://be.exceptionless.io/ vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
		"format": "prettier --plugin prettier-plugin-svelte . --write .",
		"format:staged": "prettier --plugin prettier-plugin-svelte . --write --ignore-unknown",
		"generate-models": "swagger-typescript-api -p http://localhost:5200/docs/v2/swagger.json -o ./src/lib/models -n api.generated.ts --no-client --templates api-templates",
		"generate-templates": "swagger-typescript-api generate-templates -o api-templates",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@playwright/test": "^1.37.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.24.1",
		"@sveltejs/vite-plugin-svelte": "^2.4.5",
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"autoprefixer": "^10.4.15",
		"cross-env": "^7.0.3",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"postcss": "^8.4.29",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"swagger-typescript-api": "^13.0.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vitest": "^0.34.4"
	},
	"dependencies": {
		"@iconify-json/mdi": "^1.1.54",
		"@tanstack/svelte-query": "^4.35.0",
		"@tanstack/svelte-table": "^8.9.7",
		"@web3-storage/parse-link-header": "^3.1.0",
		"class-validator": "^0.14.0",
		"daisyui": "^3.7.3",
		"oidc-client-ts": "^2.2.5",
		"svelte-infinite-scroll": "^2.0.1",
		"svelte-local-storage-store": "^0.6.0",
		"svelte-time": "^0.8.0",
		"tailwindcss": "^3.3.3",
		"unplugin-icons": "^0.17.0"
	},
	"type": "module"
}
