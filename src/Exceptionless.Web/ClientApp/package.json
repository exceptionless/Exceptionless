{
    "name": "exceptionless",
    "version": "8.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--trace-warnings vite dev",
        "dev:api": "cross-env ASPNETCORE_URLS=https://be.exceptionless.io/ vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "run-s -c test:unit test:integration",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "run-s -c lint:eslint lint:prettier",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check .",
        "format": "prettier --write .",
        "generate-models": "swagger-typescript-api -p http://localhost:5200/docs/v2/swagger.json -o ./src/lib/models -n api.generated.ts --no-client --templates api-templates",
        "generate-templates": "swagger-typescript-api generate-templates -o api-templates",
        "test:integration": "playwright test",
        "test:unit": "vitest",
        "upgrade": "ncu -i"
    },
    "devDependencies": {
        "@playwright/test": "^1.44.1",
        "@sveltejs/adapter-static": "^3.0.2",
        "@sveltejs/kit": "^2.5.16",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "@types/eslint": "^8.56.10",
        "autoprefixer": "^10.4.19",
        "cross-env": "^7.0.3",
        "eslint": "^9.5.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.39.4",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.38",
        "prettier": "^3.3.2",
        "prettier-plugin-svelte": "^3.2.4",
        "prettier-plugin-tailwindcss": "^0.6.4",
        "svelte": "^5.0.0-next.154",
        "svelte-check": "^3.8.0",
        "swagger-typescript-api": "^13.0.6",
        "tslib": "^2.6.3",
        "typescript": "^5.4.5",
        "typescript-eslint": "^8.0.0-alpha.21",
        "vite": "^5.3.1",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "@exceptionless/browser": "^3.1.0",
        "@exceptionless/fetchclient": "^0.26.0",
        "@iconify-json/mdi": "^1.1.66",
        "@tanstack/svelte-query": "^5.45.0",
        "@tanstack/svelte-query-devtools": "^5.45.0",
        "@tanstack/svelte-table": "^9.0.0-alpha.4",
        "bits-ui": "^0.21.10",
        "class-validator": "^0.14.1",
        "clsx": "^2.1.1",
        "cmdk-sv": "^0.0.17",
        "echarts": "^5.5.0",
        "formsnap": "^1.0.1",
        "mode-watcher": "^0.3.0",
        "oidc-client-ts": "^3.0.1",
        "pretty-ms": "^9.0.0",
        "svelte-echarts": "^0.1.1",
        "svelte-persisted-store": "^0.11.0",
        "svelte-sonner": "^0.3.24",
        "svelte-time": "^0.9.0",
        "sveltekit-superforms": "^2.15.1",
        "tailwind-merge": "^2.3.0",
        "tailwind-variants": "^0.2.1",
        "tailwindcss": "^3.4.4",
        "unplugin-icons": "^0.19.0"
    },
    "type": "module"
}
