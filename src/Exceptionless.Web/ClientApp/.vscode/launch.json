{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Web (Edge)",
            "url": "http://localhost:5173/next",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "npm run dev"
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Web Dev Api (Edge)",
            "url": "http://localhost:5173/next",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "npm run dev:api"
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Storybook (Edge)",
            "url": "http://localhost:6006",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "npm run storybook"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Web (Chrome)",
            "url": "http://localhost:5173/next",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "npm run dev"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Web Dev Api (Chrome)",
            "url": "http://localhost:5173/next",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "npm run dev:api"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Storybook (Chrome)",
            "url": "http://localhost:6006",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "npm run storybook"
        }
    ]
}
