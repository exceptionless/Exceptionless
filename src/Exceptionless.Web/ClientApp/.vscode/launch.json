	"version": "0.2.0",
	"configurations": [
		{
			"type": "msedge",
			"request": "launch",
			"name": "Web (Edge)",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}",
			"preLaunchTask": "npm run dev"
		},
		{
			"type": "msedge",
			"request": "launch",
			"name": "Web with Api (Edge)",
			"url": "http://localhost:5173",
