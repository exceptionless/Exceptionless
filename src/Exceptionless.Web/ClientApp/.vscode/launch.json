		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Web with Api (Chrome)",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}",
			"preLaunchTask": "npm run dev:api"
		}
